{
    "display": "Default",
    "compilerOptions": {
        "lib": ["ESNext"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "@necode-org/*": ["../*/src"],
            "*": ["../*/src"]
        },
    },
}