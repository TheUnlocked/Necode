{
    "display": "NextJS",
    "extends": "./react.json",
    "compilerOptions": {
        "allowJs": true,
        "noEmit": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "experimentalDecorators": true,
        "removeComments": true,
        "incremental": true,
    },
    "include": [
        "next-env.d.ts",
        "src"
    ],
    "exclude": ["node_modules"]
}